<h3><a href="/pets/">Pet Index</a></h3>
<h4>Pets</h4>

<% @pets.each do |pet| %>
  <h3><a href="/pets/<%=pet.id%>"><%= pet.name %></a></h3>
  <h3><%= pet.age %></h3>
  <h3><%= pet.sex %></h3>
  <img src= "<%=pet.image%>" alt="Cute Golden Retriever">
<% end %>
<p><a href="/shelters/<%= @shelters.id %>/pets/new">Create Pet</a></p>